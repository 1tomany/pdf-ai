{
    "name": "1tomany/pdf-ai",
    "description": "A simple PHP library that makes extracting data from PDFs for large language models easy",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "ai",
        "llm",
        "pdf",
        "pdf2img",
        "pdf2image",
        "pdf2text",
        "pdf2txt",
        "pdfinfo",
        "pdftoppm",
        "pdf to image",
        "pdf to text",
        "poppler",
        "xpdf"
    ],
    "require": {
        "php": ">=8.2",
        "psr/container": "^2.0",
        "symfony/filesystem": "^7.2",
        "symfony/process": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87.2",
        "phpstan/phpstan": "^2.1.23",
        "phpunit/phpunit": "^12.3.10"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\PDFAI\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\PDFAI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
